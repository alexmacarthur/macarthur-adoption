{
  "name": "macarthur-adoption",
  "version": "0.0.0",
  "license": "GPL-2.0",
  "author": "Alex MacArthur <alex@macarthur.me>",
  "description": "Our adoption website.",
  "scripts": {
    "webpack": "NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=webpack.config.js",
    "build": "NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=webpack.config.js && prettier assets/js/* --write && jekyll build && npm run minify && npm run critical && netlify-lambda build lambda-src",
    "watch": "NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=webpack.config.js --watch",
    "lambda-serve": "netlify-lambda serve lambda-src",
    "minify": "html-minifier --input-dir ./_site --output-dir ./_site --collapse-whitespace --file-ext html",
    "critical": "node critical.js",
    "jekyll": "concurrently --kill-others \"npm run watch\" \"jekyll serve\""
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "autoprefixer": "^9.5.1",
    "concurrently": "^3.5.0",
    "critical": "^1.1.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-minifier": "^3.5.7",
    "mini-css-extract-plugin": "^0.6.0",
    "netlify-lambda": "^1.4.7",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.17.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack-cli": "^3.3.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "@sendgrid/mail": "^6.3.1",
    "airtable": "^0.7.2",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "dotenv": "^6.1.0",
    "intersection-observer": "^0.5.0",
    "jump.js": "^1.0.2",
    "lozad": "^1.9.0",
    "normalize.css": "^8.0.0",
    "stripe": "^6.12.1",
    "turbolinks": "^5.2.0",
    "uuid": "^3.3.2",
    "webpack": "^4.30.0"
  }
}
